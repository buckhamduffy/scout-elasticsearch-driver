{
    "name": "buckhamduffy/scout-elasticsearch-driver",
    "type": "library",
    "description": "The Elasticsearch Driver for Laravel Scout",
    "keywords": [
        "elastic",
        "elasticsearch",
        "driver",
        "engine",
        "laravel",
        "scout",
        "search"
    ],
    "homepage": "https://babenkoivan.github.io/scout-elasticsearch-driver/",
    "license": "MIT",
    "authors": [
        {
            "name": "Babenko Ivan",
            "email": "babenko.i.a@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Aaron Florey",
            "email": "aaron@buckhamduffy.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "elasticsearch/elasticsearch": ">=7.0 <8",
        "laravel/scout": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "mockery/mockery": "^1.0",
        "buckhamduffy/coding-standards": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "ScoutElastic\\Tests\\": "tests/",
            "ScoutElastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScoutElastic\\ScoutElasticServiceProvider"
            ]
        }
    }
}
